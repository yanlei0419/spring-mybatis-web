    <profiles>
        <profile>
            <id>sqlite</id>
            <properties>
                <!--EL表达式变量   ${env}-->
                <env>sqlite</env>
                <profiles.active>sqlite</profiles.active>
            </properties>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
        </profile>
        <profile>
            <id>ora</id>
            <properties>
                <env>ora</env>
            </properties>
        </profile>
    </profiles>

    <build>
        <finalName>spring-mybatis-web</finalName>
        <!--资源文件-->
        <resources>
            <resource>
                <directory>src/main/resources</directory>
                <!-- 资源根目录排除各环境的配置，防止在生成目录中多余其它目录 -->
                <excludes>
                    <exclude>ve/**</exclude>
                </excludes>
            </resource>
            <resource>
                <directory>src/main/resources/ve/${env}</directory>
                <includes>
                    <include>jdbc.properties</include>
                </includes>
                <targetPath>.</targetPath>
            </resource>
            <resource>
                <directory>src/main/resources/ve/${env}/mapper</directory>
                <includes>
                    <include>**/**.xml</include>
                </includes>
                <!--上面文件输出的位置-->
                <targetPath>mybatis/mapper/</targetPath>
            </resource>
        </resources>

        <plugins>
            <!-- 测试插件 -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.18.1</version>
                <configuration>
                    <skipTests>false</skipTests>
                </configuration>
            </plugin>
            <!--maven  java  compiler   java  编译插件  字符集-->
